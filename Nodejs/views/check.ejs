<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Check</title>
    <link rel="stylesheet" href="../try.css" />
</head>

<body>
    <div class="header">
        <div class="logo">
          <div class="header__back">
            <i class="fas fa-angle-left"></i>
          </div>
          <h2>Checking your portrait Step</h2>
        </div>
      </div>  
    
    <div class="guideline-steps">
        <p class="step">Checking your portrait image before drawing</p>
        <hr>
        <b>GENERATOR SET 1</b>
        <% Object.values(showImage1).forEach(function(item){%>
            <p>=> <%= item.name %></p>
            <img src="<%= item.image %>">
        <% }) %>  
        <!-- <hr> -->
        <!-- <b>If generated picture is OK please click to start drawing process</b> -->
        <a href= <%= link1 %> >Start Drawing with generator set 1!!!</a>
        <hr>

        <b>GENERATOR SET 2</b>
        <% Object.values(showImage2).forEach(function(item){%>
          <p>=> <%= item.name %></p>
          <img src="<%= item.image %>">
        <% }) %>  
        <a href= <%= link2 %> >Start Drawing with generator set 2!!!</a>
        <hr>

        <b>If generated picture isn't good enough please re-upload again</b>
        <a href="/upload" class="upload-button">Re-Upload</a>
        
    </div>
    
</body>
</html>